---
layout: base.njk
pagination:
  data: days
  size: 1
  alias: item
permalink: "{{ item.permalink }}/"
eleventyComputed:
  title: "{{ item.camera.label }} Archive"
---



<h1>{{ item.camera.label }} Archive for ((placeholder month))</h1>
  <div class="grid">
{% for image in item.images | reverse %}

  <section>
    <article>
      <div class="item">
  
  <h2><a href="/{{ item.camera.id }}/{{ image.image.LastModified | date("yyyy/MM/dd") }}/">{{ image.image.LastModified | date("dd") }}</a></h2>
  <a href="/{{ item.camera.id }}/{{ image.image.LastModified | date("yyyy/MM/dd") }}/">
  <img src="{{ site.image_cdn }}{{ image.image.Key }}" alt="Latest image from {{ image.image.LastModified | date("yyyy/MM/dd") }} for {{ item.camera.label }}">
  </a>
      </div>
    </article>
  </section>
{% endfor %}

</div>